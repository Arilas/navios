{
  "name": "navios",
  "version": "0.3.0-alpha.0",
  "author": {
    "name": "Oleksandr Hanzha",
    "email": "alex@granted.name"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "tsup": {
    "entry": [
      "./src/index.mts"
    ],
    "sourcemap": true,
    "dts": true,
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true
  }
}
